{
  "name": "@white-block/resolver",
  "version": "1.3.0",
  "description": "unplugin-vue-components resolver for white-block",
  "keywords": [
    "white-block",
    "unplugin vue components",
    "resolver"
  ],
  "author": {
    "name": "ephemeras",
    "email": "ephemeras@qq.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup"
  },
  "devDependencies": {
    "unplugin-vue-components": "^0.25.2"
  }
}